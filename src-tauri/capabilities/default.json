{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "cloudv2ray:default",
    {
      "identifier": "http:default",
      "allow": [
        "https://*.tencentcloudapi.com",
        "http://*:2081"
      ]
    },
    "core:event:default",
    "store:default"
  ]
}